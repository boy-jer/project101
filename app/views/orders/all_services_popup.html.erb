<div id="services_popup">

<h1>Services</h1>
<table id="individualService" width="100%">
<% @services.in_groups_of(3, false) do |row_services| %>
  <tr>
    <% for service in row_services %>
      <td><%= link_to service.name, company_add_service_to_order_path(current_company, service), :class => "smallbtn green" %></td>
    <% end %>
  </tr>
<% end %>
</table>
	


</div>